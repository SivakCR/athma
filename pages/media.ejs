               <div class="tab-content mt-5 pt-5">
                  <div id="tab-one" class="row tab-pane fade active show " role="tabpanel">
                     <% for(var i=0; i < data.length; i++) { %>
                        <% if(data[i].type=="video") { %>
                        <div class="col-md-12">
                           <div class="card border m-1 p-2">
                              <div class="row">
                                 
                                    <div class="col-sm-6">
                                       <iframe width="100%" height="200" src="<%= data[i].url %>" frameborder="0" 
                                       allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" 
                                       allowfullscreen></iframe>
                                    </div>
                                    <div class="col-sm-6 p-4">
                                       <h2 class="text-lh-xs"><%= data[i].title %></h2>
                                       <span class="badge badge-pill mb-3 bg-primary"><%= data[i].date.toDateString() %></span>
                                    </div>
                                 
                              </div>
                           </div>
                        </div>
                        <% } %>
                        <% } %>

                  </div>
                  <div id="tab-two" class="row align-items-lg-center tab-pane fade " role="tabpanel">
                     <% for(var i=0; i < data.length; i++) { %>
                        <% if(data[i].type=="audio") { %>
                     <div class="col-md-12">
                        <div class="card p-3">
                           <div class="row">
                              
                              <div class="col-md-6">
                                 <h2 class="text-lh-xs"><%= data[i].title %></h2>
                                 <span class="badge badge-pill mb-3 bg-primary"><%= data[i].date.toDateString() %></span>
                              </div>
                              <div class="col-md-6 p-2">
                                 <audio controls>
                                    <source src="<%= data[i].url %>" type="audio/ogg">
                                    <source src="<%= data[i].url %>" type="audio/mp3">
                                    Your browser does not support the audio tag.
                                 </audio>
                              </div>
                              
                           </div>
                        </div>
                     </div>
                     <% } %>
                     <% } %>

                  </div>
               </div>
            </div>
