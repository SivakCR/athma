       <script>
        window.addEventListener("DOMContentLoaded", () => {
          const firebaseConfig = {
            apiKey: "AIzaSyBP_mWXwC0K6xV0RdO9RHI2QIVM98AXpoo",
            authDomain: "athmakalinyoga.firebaseapp.com",
            databaseURL: "https://athmakalinyoga.firebaseio.com",
            projectId: "athmakalinyoga",
            storageBucket: "athmakalinyoga.appspot.com",
            messagingSenderId: "744540009922",
            appId: "1:744540009922:web:d6ccf3095b9a6a34d4d25b",
            measurementId: "G-TR86W9DKQG"
          };

          firebase.initializeApp(firebaseConfig);

          firebase.auth().setPersistence(firebase.auth.Auth.Persistence.NONE);

          document
            .getElementById("login")
            .addEventListener("submit", (event) => {
              event.preventDefault();
              const login = event.target.login.value;
              const password = event.target.password.value;

              firebase
                .auth()
                .signInWithEmailAndPassword(login, password)
                .then(({ user }) => {
                  return user.getIdToken().then((idToken) => {
                    return fetch("/sessionLogin", {
                      method: "POST",
                      headers: {
                        Accept: "application/json",
                        "Content-Type": "application/json",
                        "CSRF-Token": Cookies.get("XSRF-TOKEN"),
                      },
                      body: JSON.stringify({ idToken }),
                    });
                  });
                })
                .then(() => {
                  return firebase.auth().signOut();
                })
                .then(() => {
                  window.location.assign("/dash");
                })
                .catch((err)=>alert(err))
              return false;
            });
        });
      </script>
      
      
