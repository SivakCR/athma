         <script>
         var gals=0;
            function more(){
               gals=gals+18;
               var v = '/galpage-'+gals.toString()
               console.log(v)
               fetch(v)
               .then((res)=>{ return(res.json()) })
               .then((res) =>{
                  console.log(res.data,res.data.length)
                  
                  for( var i=0 ;i<res.data.length;i++){
                     var d = document.getElementById("gallery-row")
                     d.innerHTML = d.innerHTML +  `<div class="col-md-4 col-md mb-2"> <a class="venobox hover-effect d-block vbox-item" data-overlay="rgba(52,58,64,.6)" href="${res.data[i].image}"> <span class="hover-effect-container"> <span class="hover-effect-icon "> <span class=" fas fa-eye top-icon "></span> </span> </span> <div style="background-image:url('${res.data[i].image}');" class="gall gall-bg-img"> </div> </a> </div>`
                  }
                  if(res.data.length==0){
                     document.getElementById('page-btn').innerHTML= "Bottom Reached"
                     document.getElementById('page-btn').setAttribute('onclick',"")
                  }
               })
                  
            }
