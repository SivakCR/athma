       <script>
         $(document).ready(function(){
            $("#btamil").click(function(){
              $("#english-event").hide();
              $("#tamil-event").show();
              
              $("#btamil").removeClass("btn-secondary");
              $("#benglish").removeClass("btn-primary");

              $("#btamil").addClass("btn-primary");
              $("#benglish").addClass("btn-secondary");

            });
            $("#benglish").click(function(){
               $("#english-event").show();
               $("#tamil-event").hide();
                             
              $("#btamil").removeClass("btn-primary");
              $("#benglish").removeClass("btn-secondary");
              
              $("#btamil").addClass("btn-secondary");
              $("#benglish").addClass("btn-primary");

             });

          });


         function evt(){
            document.getElementById("english-event").style.display="none";
            document.getElementById("tamil-event").style.display="block";
         }
         function eve(){
            document.getElementById("english-event").style.display="block";
            document.getElementById("tamil-event").style.display="none";
         }
